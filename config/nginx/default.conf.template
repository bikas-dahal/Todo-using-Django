# upstream for uWSGI

upstream uwsgi_app {
    server unix:/code/todo/uwsgi_app.sock;
}

server {
    listen       8001;
    server_name  www.mysite.com mysite.com;
    error_log    stderr warn;
    access_log   /dev/stdout main;
    location / {
        include      /etc/nginx/uwsgi_params;
        uwsgi_pass   uwsgi_app;
    }
    location /static/ {
        alias /code/todo/static/;
    }
    location /media/ {
        alias /code/todo/media/;
    }
}

